all: ${TARGS}

clean:
	rm -rf *.o *.[${O}] [$(OS)].out
	rm -rf ${TARGS}

nuke: clean

	
$(TARGS): $(TARGS:%=%.go)
	${GC} $@.go
	${LD} -o $@ $@.${O}


